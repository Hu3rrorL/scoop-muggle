{
    "version": "1.7.0.0",
    "description": "支持快捷键、不规则截图，具备放大镜功能",
    "homepage": "https://www.fiastarta.com/PrtScr/",
    "license": "Freeware",
    "url": "https://github.com/HueLiu/Binary/raw/master/utils/PrtScr_v1.7.0.0.7z#dl.7z_",
    "hash": "9b69632daf4a56de7d65e8ef8b70821702ff58edfa351346fa0ac7b38e8ec957",
    "pre_install": [
        "$7z = ( Get-ChildItem \"$dir\\PrtScr*\").Name",
        "7z x \"$dir\\$7z\" -pmuggle $('-o' + $dir) | Out-Null",
        "Move-Item \"$dir\\PrtScr\\*\" $dir",
        "Remove-Item \"$dir\\*dl.7z_\", \"$dir\\PrtScr\\\" -Force -Recurse"
    ],
    "notes": [
        "Please read ReadME.md in $dir",
        "-----",
        "请阅读 $dir 文件夹中的 `使用说明.md`"
    ],
    "shortcuts": [
        [
            "PrtScr.exe",
            "PrtScr"
        ]
    ]
}
